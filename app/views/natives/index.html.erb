<p id="notice"><%= notice %></p>

<h1>Natives</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Locations</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @natives.each do |native| %>
      <tr>
        <td><%= native.name %></td>
        <td><%= native.location.name %></td>
        <td><%= link_to 'Show', native %></td>
        <td><%= link_to 'Edit', edit_native_path(native) %></td>
        <td><%= link_to 'Destroy', native, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Native', new_native_path %>
